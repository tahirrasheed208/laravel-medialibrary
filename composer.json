{
    "name": "tahirrasheed208/laravel-medialibrary",
    "description": "Media Library for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Tahir Rasheed",
            "email": "tahirrasheed208@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/view": "^11.0",
        "intervention/image-laravel": "^1.3"
    },
    "require-dev": {
        "ext-imagick": "*",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "TahirRasheed\\MediaLibrary\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TahirRasheed\\MediaLibrary\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TahirRasheed\\MediaLibrary\\MediaLibraryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
